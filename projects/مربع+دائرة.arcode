تعريف دائرة(حجم, عدد_الاضلاع) {
    كرر عدد_الاضلاع مرات {
        تقدم(حجم);
        يمين(360 / عدد_الاضلاع);
    };
};

غيّر_لون_القلم("أحمر");
دائرة(50, 4); # مربع

ارفع_القلم();
اذهب_إلى(50, 50);
انزل_القلم();

غيّر_لون_القلم("أزرق");
دائرة(20, 8); # ثماني الأضلاع صغير

اطبع("تم رسم الأشكال بنجاح!");